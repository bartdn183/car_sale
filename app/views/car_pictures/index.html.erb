<p id="notice"><%= notice %></p>

<h1>Car Pictures</h1>

<table>
  <thead>
    <tr>
      <th>Car</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_pictures.each do |car_picture| %>
      <tr>
        <td><%= car_picture.car_id %></td>
        <td><%= link_to 'Show', car_picture %></td>
        <td><%= link_to 'Edit', edit_car_picture_path(car_picture) %></td>
        <td><%= link_to 'Destroy', car_picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Picture', new_car_picture_path %>
